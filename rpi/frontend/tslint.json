{
  "extends": "tslint-microsoft-contrib",
  "rules": {
    "trailing-comma": [
      true,
      {
        "multiline": "always"
      }
    ],
    "export-name": false,
    "no-import-side-effect": false,
    "no-parameter-properties": true,
    "no-require-imports": false,
    "no-any": false
  }
}